<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<HBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="springfx.controls.ControlsPresenter" alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="10.0">
     <children>
     	<Button fx:id="emailButton" onAction="#mail" text="Email" />
        <Button mnemonicParsing="false" onAction="#newContact" text="New" />
        <Button fx:id="deleteButton" mnemonicParsing="true" onAction="#delete" text="Delete" />
     </children>
     <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
     </padding>
</HBox>

