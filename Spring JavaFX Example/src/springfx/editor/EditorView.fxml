<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.stage.FileChooser?>


<GridPane hgap="5.0" vgap="5.0" alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" 
		fx:controller="springfx.editor.EditorController">
		
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints halignment="RIGHT" hgrow="NEVER" minWidth="10.0" />
      <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <children>
      <Label text="First Name:" />
      <Label text="Last Name:" GridPane.rowIndex="1" />
      <TextField fx:id="firstNameTextField" GridPane.columnIndex="1" onAction="#addUpdate" />
      <TextField fx:id="lastNameTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" onAction="#addUpdate" />
      <Label text="Email:" GridPane.rowIndex="2" />
      <TextField fx:id="emailTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" onAction="#addUpdate" />
      <Button fx:id="addUpdateButton" mnemonicParsing="false" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3" onAction="#addUpdate" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
